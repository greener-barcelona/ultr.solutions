{
  "version": 2,
  "outputDirectory": "public",
  "redirects": [
    { "source": "/", "destination": "/LogIn/", "permanent": false }
  ],
  "rewrites": [
    { "source": "/LogIn/:path*", "destination": "/LogIn/index.html" },
    {
      "source": "/:path*",
      "destination": "/Brainstorming/index.html"
    }
  ]
}
