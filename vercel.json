{
  "version": 2,
  "redirects": [
    { "source": "/", "destination": "/LogIn/", "permanent": false }
  ],
  "rewrites": [
    { "source": "/LogIn/:path*", "destination": "/LogIn/:path*" },
    { "source": "/(Brainstorming|Naming|Socialstorming|Briefer)/:path*", "destination": "/Brainstorming/:path*" }
  ]
}